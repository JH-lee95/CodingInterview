x=int(input())

d=[0]*x

for i in range(2,x+1):
  d[i]=d[i-1]+1 #현재 수에서 1를 빼주는 경우를 미리 가정 , d[0]=0,d[1]=0,d[2]=1,d[3]=2,d[4]=3,d[5]=4

  if i%2==0: #i가 2의 배수인 경우 
    d[i]=min(d[i],d[i//2]+1) #현재수에서 1를 빼준 경우와, 해당 숫자로 나누는 경우를 비교하여 더 작은 수(연산횟수)를 입력 d[4]=3,d[4//2]+1=2 => d[4]=2

  if i%3==0:
    d[i]=min(d[i],d[i//2]+1)

  if i%5==0:
    d[i]=min(d[i],d[i//5]+1)  #d[5]=d[4]+1=3, d[6]=min(4,6//3+1=3)==>3

print(d[x])
